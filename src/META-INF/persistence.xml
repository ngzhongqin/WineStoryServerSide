<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="winestory">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.winestory.serverside.framework.database.Entity.UserEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/winestory"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user" value="ownwinestory"/>
            <property name="eclipselink.jdbc.password" value="Abcd1234"/>
        </properties>
    </persistence-unit>
</persistence>
